(define
    (problem corridor_2a2i_2)
    (:domain corridor)
    (:agent b)

    (:init
        (assign (agent_loc b) 1)
        (assign (holding b) 1)
        (assign (hold_by i b) 0)
        (assign (hold_by j b) 1)
    )

    (:goal (and
        (= (@ep (" b [b]") (= (hold_by i b) 0)) ep.true)
        (= (@ep (" b [b]") (= (hold_by j b) 0)) ep.true)
        (= (@ep (" b [b]") (= (hold_by i a) 0)) ep.true)
        (= (@ep (" b [b]") (= (hold_by j a) 0)) ep.true)
        )
    )
)